{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "create-coupoin",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 1,
      "maxLength": 255
    },
    "shop": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 1,
      "maxLength": 255
    },
    "dueDate": {
     "type": "string",
     "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
    }
  },
  "required": ["code", "shop"],
  "additionalProperties": false
}
